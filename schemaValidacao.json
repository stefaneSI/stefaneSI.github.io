{
    "$schema": "schemaValidacao.json",
    "title": "Schema Validacao",
    "array": {
      "$id": "#/properties/schema",
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "event": {
              "type": "string",
              "enum": ["teste"]
            },
            "key1": {
              "type": "object",
              "properties": {
                "key1_sub1": {
                  "type": "number",
                  "enum": [10]
                },
                "key1_sub2": {
                  "type": "string",
                  "pattern": "teste|test|.*"
                },
                "key1_sub3": {
                  "type": "string",
                  "enum": ["producao"]
                },
                "key1_sub4": {
                  "type": "boolean",
                  "enum": "desktop|mobile|msite"
                }
              },
              "required": ["key1_sub1", "key1_sub2", "key1_sub3", "key1_sub4"]
            }
          },
          "required": ["event"]
        }
      ]
    }
  }